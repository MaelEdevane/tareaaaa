<script type="text/javascript">

    window.WhistleOut = window.WhistleOut || {};
    var WhistleOut = window.WhistleOut;

    if (!WhistleOut.isTargetingCookieEnabled) {
        WhistleOut.isTargetingCookieEnabled = function () {

            var regexp = new RegExp("OptanonConsent=([^;]+)");
            var oneTrustCookie = regexp.exec(document.cookie);
            if (!oneTrustCookie) {
                return true;
            }

            var oneTrustCookieValue = unescape(oneTrustCookie[1]); 
            if (oneTrustCookieValue.indexOf('C0004:1') >= 0) {
                return true;
            } else {
                return false;
            }

        }
    };

    if (!WhistleOut.insertScriptFile) {
        WhistleOut.insertScriptFile = function(fileUrl) {
            if (!WhistleOut.isTargetingCookieEnabled()) {
                return;
            }

            var placeHolder = document.getElementById('remarketing-scripts-placeholder');
            if (!placeHolder) {
                throw 'Error, no javascript place holder is found.';
                return;
            }

            var scriptElement = document.createElement('script');
            scriptElement.src = fileUrl;
            placeHolder.appendChild(scriptElement);
        };
    }

</script>

